{
    "name": "unicode-unicorn",
    "version": "1.0.0",
    "description": "A JavaScript Unicode IDE",
    "private": true,
    "dependencies": {
        "@popperjs/core": "^2.11.4",
        "bootstrap": "^5.1.3",
        "jquery": "^3.6.0",
        "typescript": "^4.6.2",
        "util": "^0.12.4"
    },
    "devDependencies": {
        "@types/bootstrap": "^5.1.9",
        "@types/jquery": "^3.5.14",
        "@wasm-tool/wasm-pack-plugin": "^1.6.0",
        "copy-webpack-plugin": "^10.2.4",
        "css-loader": "^6.7.1",
        "mini-css-extract-plugin": "^2.6.0",
        "prettier": "^2.6.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.8",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.7.4"
    },
    "scripts": {
        "clean": "rm -r dist wasm/pkg wasm/target",
        "build": "node data/compile-unicode-data.js && webpack",
        "start": "node data/compile-unicode-data.js && webpack serve --mode=development"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/printfn/unicode-unicorn.git"
    },
    "author": "printfn",
    "license": "AGPL-3.0",
    "bugs": {
        "url": "https://github.com/printfn/unicode-unicorn/issues"
    },
    "homepage": "https://github.com/printfn/unicode-unicorn#readme",
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "printWidth": 100,
        "overrides": [
            {
                "files": "**/*.yml",
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    },
    "packageManager": "yarn@3.2.3"
}
