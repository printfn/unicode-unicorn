{
    "name": "unicode-unicorn",
    "version": "1.0.0",
    "description": "A JavaScript Unicode IDE",
    "private": true,
    "dependencies": {
        "@popperjs/core": "^2.11.8",
        "@types/node": "^20.4.6",
        "bootstrap": "^5.3.1",
        "ts-node": "^10.9.1",
        "typescript": "~5.1.6"
    },
    "devDependencies": {
        "@types/bootstrap": "^5.2.6",
        "prettier": "^3.0.1",
        "ts-loader": "^9.4.4",
        "vite": "^5.0.10"
    },
    "scripts": {
        "clean": "rm -r dist wasm/pkg wasm/target",
        "build": "(cd wasm && wasm-pack build -t web) && ts-node -O '{\"module\": \"commonjs\"}' data/compile-unicode-data.ts && vite build",
        "start": "(cd wasm && wasm-pack build -t web) && ts-node -O '{\"module\": \"commonjs\"}' data/compile-unicode-data.ts && vite",
        "server": "vite preview"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/printfn/unicode-unicorn.git"
    },
    "author": "printfn",
    "license": "AGPL-3.0",
    "bugs": {
        "url": "https://github.com/printfn/unicode-unicorn/issues"
    },
    "homepage": "https://github.com/printfn/unicode-unicorn#readme",
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "printWidth": 100,
        "overrides": [
            {
                "files": "**/*.yml",
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    }
}
