{
    "name": "unicode-unicorn",
    "version": "1.0.0",
    "description": "A JavaScript Unicode IDE",
    "private": true,
    "dependencies": {
        "@popperjs/core": "^2.11.8",
        "@types/node": "^20.4.6",
        "bootstrap": "^5.3.1",
        "ts-node": "^10.9.1",
        "typescript": "~5.1.6"
    },
    "devDependencies": {
        "@types/bootstrap": "^5.2.6",
        "@wasm-tool/wasm-pack-plugin": "^1.7.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "mini-css-extract-plugin": "^2.7.6",
        "prettier": "^3.0.1",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.4.4",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "scripts": {
        "clean": "rm -r dist wasm/pkg wasm/target",
        "build": "ts-node -O '{\"module\": \"commonjs\"}' data/compile-unicode-data.ts && webpack",
        "start": "ts-node -O '{\"module\": \"commonjs\"}' data/compile-unicode-data.ts && webpack serve --mode=development"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/printfn/unicode-unicorn.git"
    },
    "author": "printfn",
    "license": "AGPL-3.0",
    "bugs": {
        "url": "https://github.com/printfn/unicode-unicorn/issues"
    },
    "homepage": "https://github.com/printfn/unicode-unicorn#readme",
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "printWidth": 100,
        "overrides": [
            {
                "files": "**/*.yml",
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    },
    "packageManager": "yarn@3.6.1"
}
